{
    "manifest_version": 3,
    "name": "changeUrl",
    "version": "1.6",
    "description": "读取剪贴板替换URL域名",
    "incognito": "split",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "icons": {
    "16": "icons/icon48.png",
    "48": "icons/icon48.png",
    "128": "icons/icon48.png"
  },
    "action": {
      "default_title": "读取剪贴板",
      "default_icon": {
      "16": "icons/icon48.png",
    "48": "icons/icon48.png",
    "128": "icons/icon48.png"
    }
    },
    "web_accessible_resources": [
      {
        "resources": ["clipboard.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  